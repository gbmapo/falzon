{
    "type": "project",
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.6.4",
        "drupal/onepage_zymphonies_theme": "^2.0",
        "drupal/colorbox_inline": "^2.0",
        "drupal/imce": "^2.4",
        "drupal/magnific_popup": "^1.3",
        "drupal/fullcalendar_view": "^5.1",
        "drupal/devel": "^5.0",
        "drupal/backup_migrate": "^5.0",
        "drupal/colorbox": "^2.0",
        "drupal/colorbox_load": "^1.2",
        "drupal/core-recommended": "^9",
        "drupal/core-vendor-hardening": "^9.3",
        "drupal/core-dev": "^9",
        "drupal/core-composer-scaffold": "^9",
        "drupal/upgrade_status": "^3.13",
        "drush/drush": "^10.6",
        "drupal/r4032login": "^2.1"
    },
    "extra": {
        "enable-patching": true,
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "drupal-scaffold": {
            "locations": {
                "web-root": "./"
            }
        },
        "installer-paths": {
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "core": [
                "type:drupal-core"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ]
        },
        "merge-plugin": {
            "include": [
                "modules/custom/*/composer.json"
            ],
            "replace": false,
            "ignore-duplicates": true
        },
        "patches": {}
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
